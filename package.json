{
  "name": "embedded-error-board",
  "version": "0.0.2",
  "description": "Track and fix JavaScript errors fired by your visitor's browsers",
  "main": "dist/backend",
  "author": [
    {
      "name": "Aziz Yuldoshev",
      "email": "yuldoshev.aziz@gmail.com",
      "url": "http://lapple.me"
    },
    "ewnd9 <ewndnine@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/embedded-error-board.git"
  },
  "scripts": {
    "build": "npm run clean && npm run copy-assets && npm run build:backend && npm run build:frontend",
    "build:backend": "NODE_ENV=production webpack --config webpack.config.backend.js && sed -i -e '1imodule.exports = \\' dist/backend.js",
    "build:frontend": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "clean": "rm -rf dist && rm -rf public",
    "copy-assets": "mkdir public && cp src/client/assets/* public/",
    "lint": "eslint 'src/**/*.js'",
    "prepublish": "npm run build",
    "prepush": "npm run lint && npm test",
    "start": "node dist/backend",
    "start:dev": "babel-watch --exclude src/client src/cli"
  },
  "files": [
    "dist",
    "public"
  ],
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.1.0",
    "ejs": "^2.2.3",
    "envify": "^3.2.0",
    "express": "^4.11.1",
    "lodash": "^2.4.1",
    "moment": "^2.9.0",
    "morgan": "^1.7.0",
    "nedb": "^1.1.1",
    "page": "^1.3.7",
    "pify": "^2.3.0",
    "react": "^0.12.2",
    "react-tools": "^0.12.2",
    "serve-favicon": "^2.0.0",
    "sockjs": "^0.3.9",
    "superagent": "^0.21.0",
    "uglify-js": "^2.4.16",
    "useragent": "^2.0.8",
    "vow": "^0.4.8"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^2.0.2",
    "babelify": "^7.3.0",
    "browserify": "^8.1.0",
    "browserify-shim": "^3.8.2",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.3.0",
    "husky": "^0.11.3",
    "nodemon": "^1.2.1",
    "ohcrash": "^1.0.3",
    "proxyquire": "^1.7.10",
    "reactify": "^1.0.0",
    "source-map-support": "^0.4.0",
    "supertest": "^1.2.0",
    "watchify": "^2.3.0",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.2.0",
    "webpackman": "^0.5.0"
  }
}
