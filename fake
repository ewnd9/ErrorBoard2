#!/bin/sh

URL="http://localhost:8000/error-board"

curl "$URL/error?url=/home&message=MUncaught%20TypeError%3A%20Cannot%20call%20method%20'replace'%20of%20undefined" -H 'User-Agent: Mozilla/5.0 (Macintosh; Intel Mac OS X 10_9_2) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/32.0.1700.102 YaBrowser/14.2.1700.12597 Safari/537.36'
curl "$URL/error?url=/url&message=destruct%20table%3A%201%2C678ms&meta=staging" -H 'User-Agent: Mozilla/5.0 (X11; Ubuntu; Linux i686; rv:24.0) Gecko/20100101 Firefox/24.0'
curl "$URL/error?url=/checkout&message=nextension.toLowerCase%20is%20not%20a%20function&meta=%7B%22server%22%3A%22s01e01%22%7D" -H 'User-Agent: Mozilla/5.0 (compatible; MSIE 10.0; Windows NT 6.2; Trident/6.0)'
curl "$URL/error?url=/home&message=nextension.toLowerCase%20is%20not%20a%20function" -H 'User-Agent: Mozilla/4.0 (compatible; MSIE 8.0; Windows NT 6.0; Trident/4.0)'
curl "$URL/error?url=/url&message=destruct%20table%3A%201%2C99ms" -H 'User-Agent: Mozilla/5.0 (Linux; U; Android 4.0.2; en-us; Galaxy Nexus Build/ICL53F) AppleWebKit/534.30 (KHTML, like Gecko) Version/4.0 Mobile Safari/534.30'
curl "$URL/error?url=/checkout&message=Load%20the%20React%20DevTools%20for%20a%20better%20development%20experience%3A%20http%3A%2F%2Ffb.me%2Freact-devtools" -H 'User-Agent: Mozilla/5.0 (iPad; CPU OS 7_0_2 like Mac OS X) AppleWebKit/537.51.1 (KHTML, like Gecko) Version/7.0 Mobile/11A501 Safari/9537.53' -H 'Referer: /rome'
curl "$URL/error?url=/home&message=Those%20methods%20are%20used%20to%20cover%20a%20string%20or%20alphabet%20from%20lower%20case%20to%20upper%20case%20or%20vice%20versa.%20e.g%3A%20%22and%22%20to%20%22AND%22." -H 'User-Agent: Mozilla/5.0 (Windows NT 6.1; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/28.0.1500.52 Safari/537.36 OPR/15.0.1147.100'
curl "$URL/error?url=/chrome&message=chrome.returnValue%20is%20deprecated.%20Please%20use%20the%20standard%20event.preventDefault()%20instead." -H 'User-Agent: Mozilla/5.0 (Macintosh; Intel Mac OS X 10_9_2) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/36.0.1944.0 Safari/537.36'
